tab_deceased90_all <- regdata |>
  dplyr::filter(OPERATION_DATE < max(as.Date(regdata$OPERATION_DATE)) -
                  lubridate::days(110),
                operation_group != "Other") |>
  summarise(
    N =n(),
    n_mort = sum(IN_HOUSE_DEATH | IN_HOUSE_DEATH_READMISSION |
                   DEATH == 1, na.rm = T),
    unknown = sum(
      IN_HOUSE_DEATH == 0 &
        (IN_HOUSE_DEATH_READMISSION == 0 | is.na(IN_HOUSE_DEATH_READMISSION)) &
        (DEATH == 9 | is.na(DEATH))),
    .by = operation_group ) |>
  mutate(
    conf_dead = paste0(n_mort, " (", sprintf('%.1f', n_mort/N*100), "%)"),
    lost2followup = paste0(
      unknown, " (", sprintf('%.1f', unknown/N*100), "%)"),
    worstcase = paste0(
      n_mort+unknown, " (", sprintf('%.1f', (n_mort+unknown)/N*100), "%)")
  ) |>
  select(operation_group, N, conf_dead, lost2followup, worstcase)

print(xtable::xtable(
  tab_deceased90_all |>
    arrange(operation_group),
  digits=0,
  align = c("l", "L{1.5in}", rep('r', ncol(tab_deceased90_all)-1)),
  caption = "N is the number of included patients with a minimum of 110 days follow-up time. unknown is the number of patients that are lost to follow-up at 90 days (not confirmed alive or dead)."),
  include.rownames=FALSE)

